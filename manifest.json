{
  "manifest_version": 3,
  "name": "Auto Add Friend Facebook",
  "version": "1.0",
  "description": "Tự động kết bạn trên Facebook.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://www.facebook.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
